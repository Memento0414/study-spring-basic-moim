{{> /common/header}}
{{> /common/navi-bar}}
<div class="py-2 border-bottom">
	<h5 class="fs-5">#찾아낸 사용자들과 놀라운 이야기를 나눠보세요.</h5>
</div>
<form class="my-2 p-2" method="get" action="/search/result">
	<div class="d-flex justify-content-end g-3">
		<div class="px-1">
			<input type="text" class="form-control" name="keyword" value="{{#keykeyword}} {{.}} {{/keykeyword}}" />
		</div>
		<div class="px-1">
			<button class="btn btn-outline-primary form-control">검색</button>
		</div>
	</div>
</form>

<div style="align-items: center;">
		{{#findUser}}
			<ul>
				<li>{{id}} <small>({{{nick}}})</small></li>
			</ul>
		{{/findUser}}
		
	</div>
<nav aria-label="Page navigation example">
	<ul class="pagination justify-content-center">
		<li class="page-item disabled"><a class="page-link" href="#"><<</a></li> 
		{{#pages}}
			<li class="page-item"><a class="page-link" href="/search/result?keyword=&page={{.}}">{{.}}</a></li>
		{{/pages}}
		<li class="page-item"><a class="page-link" href="#">>></a></li>
	</ul>
</nav>

{{> /common/footer}}